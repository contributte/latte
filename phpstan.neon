includes:
	- vendor/contributte/phpstan/phpstan.neon

parameters:
	level: 9
	phpVersion: 80200

	scanDirectories:
		- src

	fileExtensions:
		- php

	paths:
		- src
		- .docs

	ignoreErrors:
		# ParsedownExtra is an optional dependency
		- '#unknown class ParsedownExtra#i'
		- '#invalid type ParsedownExtra#i'
		- '#Instantiated class ParsedownExtra not found#'
