{
  "name": "contributte/latte",
  "description": "Extra contrib to nette/latte",
  "keywords": [
    "nette",
    "latte",
    "extra"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/latte",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.2",
    "latte/latte": "^3.1.0"
  },
  "require-dev": {
    "contributte/phpstan": "^0.1",
    "contributte/qa": "^0.4",
    "nette/application": "^3.1.14",
    "nette/di": "^3.0.17",
    "nette/tester": "^2.5"
  },
  "suggest": {
    "nette/di": "to use VersionExtension[CompilerExtension]",
    "erusev/parsedown-extra": "to use ParsedownExtension[CompilerExtension]"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Latte\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.7.x-dev"
    }
  }
}
