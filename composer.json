{
  "name": "contributte/latte",
  "description": "Extra contrib to nette/latte",
  "keywords": [
    "nette",
    "latte",
    "extra"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/latte",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "latte/latte": "^2.5.1 || ^3.0"
  },
  "require-dev": {
    "nette/application": "^3.0",
    "nette/di": "~3.0.0",
    "ninjify/nunjuck": "^0.4",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0"
  },
  "conflict": {
    "nette/di": "<3.0.0"
  },
  "suggest": {
    "nette/di": "to use VersionExtension[CompilerExtension]"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Latte\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6.x-dev"
    }
  }
}
